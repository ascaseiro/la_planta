  <!-- NAVBAR -->
  <nav class="navbar navbar-expand-sm navbar-light bg-white" style="height: 110px;">
    <a class="navbar-brand h-100" href="/"><img src="/imagenes/logo.png" alt="La Planta Coworking - Vigo" style="max-height: 100%;"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse flex-grow-1 text-right" id="navbarNav">
      <ul class="navbar-nav ml-auto">
        <li class="nav-item">
          <a class="nav-link" href="/">Inicio</a>
        </li>
        <li class="nav-item">
